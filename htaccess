# DO NOT REMOVE THIS LINE AND THE LINES BELLOW SSL_REDIRECT:coronavirusemsc.com.br
RewriteEngine on
RewriteCond %{HTTPS} off
RewriteCond %{HTTP_HOST} (www\.)?coronavirusemsc.com.br
RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]
